{
    "name": "bakle/translator",
    "description": "Translate lang files to any defined languages with just a command.",
    "keywords": ["bakle", "translator", "laravel", "google", "google translator"],
    "type": "library",
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.2 <5.8",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": ">=5.1 <5.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bashir Akle",
            "email": "bashir.akle@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bakle\\Translator\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Bakle\\Translator\\TranslatorServiceProvider"
            ]
        }
    }
}
